{
  "name": "ease-scaffold",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_DISABLE_DEVTOOLS=1 next dev --webpack",
    "dev:turbo": "next dev --turbo",
    "build": "cross-env NEXT_DISABLE_TURBOPACK=1 NEXT_DISABLE_DEVTOOLS=1 next build",
    "build:webpack": "cross-env NEXT_DISABLE_TURBOPACK=1 NEXT_DISABLE_DEVTOOLS=1 next build",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "vitest",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "node -e \"try{require('fs').rmSync('.next',{recursive:true,force:true});require('fs').rmSync('node_modules/.cache',{recursive:true,force:true});console.log('Cleaned .next and node_modules/.cache')}catch(e){console.error(e.message)}\"",
    "reinstall": "pnpm run clean && pnpm install",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky install",
    "commitlint": "commitlint",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:status": "npx supabase status",
    "db:reset": "npx supabase db reset",
    "db:migration:new": "npx supabase migration new",
    "db:migration:up": "npx supabase db push",
    "db:migration:pull": "npx supabase db pull",
    "db:migration:diff": "npx supabase db diff",
    "db:link": "npx supabase link --project-ref jeixualvoujzhgxmouxb",
    "db:types": "npx supabase gen types typescript --linked > types/supabase.ts",
     "grant:access": "node scripts/grant-access.mjs",
     "seed:talk": "node scripts/seed-talk.mjs"
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.15",
    "@livekit/components-styles": "^1.1.6",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.77.0",
    "googleapis": "^164.1.0",
    "livekit-client": "^2.15.14",
    "livekit-server-sdk": "^2.14.0",
    "lucide-react": "^0.548.0",
    "next": "16.0.1",
    "openai": "^6.7.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.6",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "supabase": "^2.54.11",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  }
}
